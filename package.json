    "pdf-parse": "^2.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-i18next": "^13.0.1",
    "react-router-dom": "^6.11.2",
    "split-pane-react": "^0.1.3",
    "suspend-react": "^0.1.3",
