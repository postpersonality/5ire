{
  "name": "5ire",
  "productName": "5ire",
  "version": "0.15.0",
  "description": "A Sleek Desktop AI Assistant & MCP Client",
  "main": "src/main/main.ts",
  "repository": "https://github.com/nanbingxyz/5ire.git",
  "author": {
    "name": "Ironben",
    "email": "support@5ire.app",
    "url": "https://5ire.app"
  },
  "license": "Modified Apache-2.0",
  "scripts": {
    "postinstall": "patch-package && node ./node_modules/electron/install.js && npm run install:deps",
    "install:deps": "electron-builder install-app-deps",
    "lint": "biome check --write .",
    "test": "jest",
    "prepare": "husky",
    "format": "biome format --write .",
    "check": "tsc --noEmit",
    "build": "npm run install:deps && rsbuild build",
    "build:watch": "npm run install:deps && rsbuild dev",
    "dev:build": "rsbuild dev",
    "dev:start": "npx wait-on http-get://127.0.0.1:33077 file:./output/nodemon.json && cd ./output && npx nodemon",
    "dev": "rimraf ./output/ && concurrently -n R,E -c cyan,magenta \"npm run dev:build\" \"npm run dev:start\"",
    "package": "rimraf ./release && tsx ./scripts/package/index.ts",
    "package:publish": "rimraf ./release && tsx ./scripts/package/index.ts --publish",
    "generate:drizzle": "drizzle-kit generate"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src",
      "test"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "electron": "<rootDir>/test/mocks/electron.js",
      "electron-log": "<rootDir>/test/mocks/electron-log.js",
      "env.js": "<rootDir>/node_modules/@xenova/transformers/src/env.test.js"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "node",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist",
      ".erb/dll"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@axiomhq/js": "^1.0.0-rc.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
